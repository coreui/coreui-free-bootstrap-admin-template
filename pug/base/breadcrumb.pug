extends /_layout/default.pug

block view
  .c-fade-in
    .c-row
      .c-col-lg-12
        .c-card
          .c-card-header
            i.fa.fa-align-justify
            |  Bootstrap Breadcrumb
            .c-card-header-actions
              a.c-card-header-action(href='http://coreui.io/docs/components/bootstrap-breadcrumb/', target='_blank')
