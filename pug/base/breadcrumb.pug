              ol.c-breadcrumb
                li.c-breadcrumb-item
                  a(href='#') Home
                li.c-breadcrumb-item.c-active(aria-current='page') Library
            nav(aria-label='breadcrumb', role='navigation')
              ol.c-breadcrumb
                li.c-breadcrumb-item
                  a(href='#') Home
                li.c-breadcrumb-item
                  a(href='#') Library
                li.c-breadcrumb-item.c-active(aria-current='page') Data
            nav.c-breadcrumb
              a.c-breadcrumb-item(href='#') Home
